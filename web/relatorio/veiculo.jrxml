	<parameter name="idLaudo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     laudo.`localExame` AS laudo_localExame,
     laudo.`referencia` AS laudo_referencia,
     guia_pericial.`numeroGuia` AS guia_pericial_numeroGuia,
     guia_pericial.`vitima` AS guia_pericial_vitima,
     guia_pericial.`envolvido` AS guia_pericial_envolvido,
     autoridade.`nome` AS autoridade_nome,
     destino.`nomeDestino` AS destino_nomeDestino,
     vistoria_veiculo.`dataExame` AS vistoria_veiculo_dataExame,
     veiculo.`ano` AS veiculo_ano,
     veiculo.`chassis` AS veiculo_chassis,
     veiculo.`cor` AS veiculo_cor,
     veiculo.`marca` AS veiculo_marca,
     veiculo.`modelo` AS veiculo_modelo,
     veiculo.`odometro` AS veiculo_odometro,
     veiculo.`placa` AS veiculo_placa,
     veiculo.`pneu` AS veiculo_pneu,
     tipo_de_exame.`descricaoExame` AS tipo_de_exame_descricaoExame,
     procedencia.`descricao` AS procedencia_descricao,
     danos.`descricao` AS danos_descricao,
     danos.`imagem` AS danos_imagem,
     laudo.`idLaudo` AS laudo_idLaudo
FROM
     `guia_pericial` guia_pericial INNER JOIN `laudo` laudo ON guia_pericial.`idGuia` = laudo.`guia`
     INNER JOIN `autoridade` autoridade ON guia_pericial.`autoridade` = autoridade.`idAutoridade`
     INNER JOIN `destino` destino ON guia_pericial.`destino` = destino.`idDestino`
     INNER JOIN `tipo_de_exame` tipo_de_exame ON guia_pericial.`tipoExame` = tipo_de_exame.`idTipoExame`
     INNER JOIN `procedencia` procedencia ON guia_pericial.`procedencia` = procedencia.`idProcedencia`
     INNER JOIN `vistoria_veiculo` vistoria_veiculo ON laudo.`idLaudo` = vistoria_veiculo.`idVistoriaVeiculo`
     INNER JOIN `danos` danos ON laudo.`idLaudo` = danos.`laudo`
     INNER JOIN `veiculo` veiculo ON vistoria_veiculo.`veiculo` = veiculo.`idVeiculo`
WHERE
     laudo.`idLaudo` = idLaudo]]>
	</queryString>
